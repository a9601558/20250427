{
  "name": "socket-test",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:server": "cd server && npm run build",
    "build:all": "npm run build && npm run build:server && mkdir -p dist/server && cp -r server/dist/* dist/server/",
    "preview": "vite preview",
    "start": "cd server && npm start",
    "add:countdown": "cd server && npm run add:countdown",
    "deploy:full": "cd server && npm run deploy:full",
    "start:with-migrations": "cd server && npm run start:with-migrations",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:migrate:status": "sequelize-cli db:migrate:status",
    "db:migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:undo": "sequelize-cli db:seed:undo:all",
    "db:reset": "npm run db:migrate:undo:all && npm run db:migrate && npm run db:seed"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.3.0",
    "antd": "^5.24.8",
    "axios": "^1.3.6",
    "date-fns": "^4.1.0",
    "firebase": "^11.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0",
    "react-spring": "^9.7.5",
    "react-toastify": "^9.1.2",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/date-fns": "^2.5.3",
    "@types/firebase": "^2.4.32",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
